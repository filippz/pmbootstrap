Content-Transfer-Encoding: 8bit

Signed-off-by: Filip MatijeviÄ‡ <filip.matijevic.pz@gmail.com>
---
 arch/arm/boot/dts/omap3-n9.dts | 7 +++++++
 1 file changed, 7 insertions(+)

diff --git a/arch/arm/boot/dts/omap3-n9.dts b/arch/arm/boot/dts/omap3-n9.dts
index a5c1b2ceb184..517c13c8e314 100644
--- a/arch/arm/boot/dts/omap3-n9.dts
+++ b/arch/arm/boot/dts/omap3-n9.dts
@@ -11,6 +11,7 @@
 /dts-v1/;

 #include "omap3-n950-n9.dtsi"
+#include <dt-bindings/input/input.h>

 / {
	model = "Nokia N9";
@@ -72,6 +73,12 @@
	st,max-limit-z = <46>;
 };

+&twl_keypad {
+	linux,keymap = < MATRIX_KEY(6, 8, KEY_VOLUMEUP)
+			 MATRIX_KEY(7, 8, KEY_VOLUMEDOWN)
+			 >;
+};
+
 &lcd0 {
	compatible = "smd,pyrenees", "panel-dsi-cm";
 };
